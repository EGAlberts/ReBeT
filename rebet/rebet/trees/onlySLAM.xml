<root BTCPP_format="4">
  <BehaviorTree ID="Untitled">
	<Sequence>
			<initialPose topic_name="initialpose"/>
			<RetryUntilSuccessful num_attempts="-1">
			<Inverter>
			<Sequence>
				<getMap service_name="/slam_toolbox/dynamic_map" map_retrieved="{current_map}"/>
				<findFrontier service_name="/get_furthest_frontier" in_odom="{odometry}" map_to_find_frontier="{current_map}" frontier_positions="{route_poses}"/>
				<visitFrontier poses="{route_poses}" name_of_task="{current_task}"/>
			</Sequence>
			</Inverter>
			</RetryUntilSuccessful>

	</Sequence>
  </BehaviorTree>
</root>
